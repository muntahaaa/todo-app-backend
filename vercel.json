{
  "version": 2,
  "functions": {
    "api/index.js": {
      "runtime": "nodejs18.x",
      "regions": ["bom1"]
    }
  },
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/api/index.js"
    }
  ]
}
